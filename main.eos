gras:RECHTECK
gras.eckenSetzen(-960,0,960,-515)
gras.füllfarbeSetzen(Grün)
gras.randfarbeSetzen(Grün)

strasse:RECHTECK
strasse.eckenSetzen(-960,-100,960,-350)
