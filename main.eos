heliHinterteil:RECHTECK
heliHinterteil.eckenSetzen(300,40,500,20)
heliHinterteil.füllfarbeSetzen(Gelb)
heliHinterteil.randfarbeSetzen(Gelb)

heliHintererRotor1:RECHTECK
heliHintererRotor1.eckenSetzen(440,35,520,25)
heliHintererRotor1.füllfarbeSetzen(Schwarz)

heliHintererRotor2:RECHTECK
heliHintererRotor2.eckenSetzen(485,70,475,-10)
heliHintererRotor2.füllfarbeSetzen(Schwarz)

heliRotorStock:RECHTECK
heliRotorStock.eckenSetzen(135,100,145,50)
heliRotorStock.füllfarbeSetzen(Schwarz)

heliObererRotor:RECHTECK
heliObererRotor.eckenSetzen(30,108,250,100)
heliObererRotor.füllfarbeSetzen(Schwarz)

heliSchiene:RECHTECK
heliSchiene.eckenSetzen(0,-95,300,-100)
