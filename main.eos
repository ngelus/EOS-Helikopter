streifen1.randfarbeSetzen(Weiss)

heliCockpit:KREIS
heliCockpit.mittelpunktSetzen(0,0)
heliCockpit.radiusSetzen(50)
heliCockpit.füllfarbeSetzen(Weiss)
heliCockpit.randstärkeSetzen(5)
